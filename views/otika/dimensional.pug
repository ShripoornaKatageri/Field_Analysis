                      .form-group
                        label
                          | Roller Height 
                      form.form-inline
                        label.sr-only(for='inlineFormInputGroupUsername2') Roller Height 1 
                        .input-group.mb-2.mr-sm-2
                          input#inlineFormInputGroupUsername2.form-control(type='text' placeholder='Roller Height 1')
                        label.sr-only(for='inlineFormInputGroupUsername2') Roller Height 2 
                        .input-group.mb-2.mr-sm-2
                          input#inlineFormInputGroupUsername2.form-control(type='text' placeholder='Roller Height 2')
                        label.sr-only(for='inlineFormInputGroupUsername2') Roller Height 3
                        .input-group.mb-2.mr-sm-2
