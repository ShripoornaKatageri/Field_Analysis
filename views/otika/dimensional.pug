                          input#inlineFormInputGroupUsername2.form-control(type='text' placeholder='Slot Width 1')
                        label.sr-only(for='inlineFormInputGroupUsername2') Slot Width 2 
                        .input-group.mb-2.mr-sm-2
                          input#inlineFormInputGroupUsername2.form-control(type='text' placeholder='Slot Width 2')
                        label.sr-only(for='inlineFormInputGroupUsername2') Slot Width 3
